Cell[BoxData[
 RowBox[{"\n", 
  RowBox[{"(*", 
   RowBox[{
   "This", " ", "segment", " ", "Calculates", " ", "sroots", " ", "with", " ",
     "the", " ", 
    RowBox[{"rdot", "^", "2"}], " ", "square", " ", "values", " ", "as", " ", 
    "input", " ", "already"}], "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"aarray", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"2", " ", "En"}], "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"3", " ", 
           RowBox[{"En", "^", "2"}], " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "1"}], "+", 
             RowBox[{"3", " ", "\[Eta]"}]}], ")"}]}], ")"}], "/", 
         RowBox[{"c", "^", "2"}]}]}], ",", 
       RowBox[{"2", "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"2", " ", "En", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "6"}], "+", 
             RowBox[{"7", " ", "\[Eta]"}]}], ")"}]}], ")"}], "/", 
         RowBox[{"c", "^", "2"}]}]}], ",", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"h", "^", "2"}]}], "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"2", " ", "En", " ", 
            RowBox[{"h", "^", "2"}], " ", 
            RowBox[{"(", 
             RowBox[{"1", "-", 
              RowBox[{"3", " ", "\[Eta]"}]}], ")"}]}], "+", 
           RowBox[{"5", " ", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"-", "2"}], "+", "\[Eta]"}], ")"}]}]}], ")"}], "/", 
         RowBox[{"c", "^", "2"}]}]}], ",", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"h", "^", "2"}], " ", 
          RowBox[{"(", 
           RowBox[{"8", "-", 
            RowBox[{"3", " ", "\[Eta]"}]}], ")"}]}], ")"}], "/", 
        RowBox[{"c", "^", "2"}]}]}], "}"}]}], ";"}], "\n", "\n", "\n", 
   RowBox[{
    RowBox[{"aarray2", "=", 
     RowBox[{"FullSimplify", "[", 
      RowBox[{"aarray", "/", 
       RowBox[{"aarray", "[", 
        RowBox[{"[", 
         RowBox[{"-", "1"}], "]"}], "]"}]}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"spoly", "=", 
     RowBox[{"Series", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"s", "-", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"1", "+", 
             RowBox[{"Sqrt", "[", 
              RowBox[{"1", "+", 
               RowBox[{"2", " ", 
                RowBox[{"h", "^", "2"}], " ", "En"}]}], "]"}]}], ")"}], "/", 
           RowBox[{"h", "^", "2"}]}], " ", "-", 
          RowBox[{"u1", "/", 
           RowBox[{"c", "^", "2"}]}]}], ")"}], 
        RowBox[{"(", 
         RowBox[{"s", "-", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"1", "-", 
             RowBox[{"Sqrt", "[", 
              RowBox[{"1", "+", 
               RowBox[{"2", " ", 
                RowBox[{"h", "^", "2"}], " ", "En"}]}], "]"}]}], ")"}], "/", 
           RowBox[{"h", "^", "2"}]}], " ", "-", 
          RowBox[{"u2", "/", 
           RowBox[{"c", "^", "2"}]}]}], ")"}], 
        RowBox[{"(", 
         RowBox[{"s", "-", "u3", "-", 
          RowBox[{"u4", "*", 
           RowBox[{"c", "^", "2"}]}]}], ")"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"s", ",", "0", ",", "3"}], "}"}]}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"sarray", "=", 
     RowBox[{"coefficientsFromPolynomial", "[", 
      RowBox[{"spoly", ",", "s"}], "]"}]}], ";"}], "\n", "\n", 
   RowBox[{
    RowBox[{"m", "=", "1"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"eqns", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"aarray2", "[", 
         RowBox[{"[", "i", "]"}], "]"}], "==", 
        RowBox[{"sarray", "[", 
         RowBox[{"[", "i", "]"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", 
         RowBox[{"m", "+", "2"}]}], "}"}]}], "]"}]}], ";"}], "\n", "\n", 
   RowBox[{
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", "=", "1"}], ",", 
      RowBox[{"i", "<=", 
       RowBox[{"m", "+", "2"}]}], ",", 
      RowBox[{"i", "++"}], ",", "\n", 
      RowBox[{
       RowBox[{
        RowBox[{"eqns", "[", 
         RowBox[{"[", "i", "]"}], "]"}], "=", 
        RowBox[{"MapThread", "[", 
         RowBox[{"Equal", ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"coefficientsFromPolynomial", "[", 
             RowBox[{
              RowBox[{"removeNegativeOrderTermsTillOrderN", "[", 
               RowBox[{
                RowBox[{"FullSimplify", "[", 
                 RowBox[{"eqns", "[", 
                  RowBox[{"[", 
                   RowBox[{"i", ",", "1"}], "]"}], "]"}], "]"}], ",", "c", 
                ",", 
                RowBox[{"2", "*", "m"}]}], "]"}], ",", "c"}], "]"}], ",", 
            RowBox[{"coefficientsFromPolynomial", "[", 
             RowBox[{
              RowBox[{"removeNegativeOrderTermsTillOrderN", "[", 
               RowBox[{
                RowBox[{"FullSimplify", "[", 
                 RowBox[{"eqns", "[", 
                  RowBox[{"[", 
                   RowBox[{"i", ",", "2"}], "]"}], "]"}], "]"}], ",", "c", 
                ",", 
                RowBox[{"2", "*", "m"}]}], "]"}], ",", "c"}], "]"}]}], 
           "}"}]}], "]"}]}], ";"}]}], "\n", "]"}], ";"}], "\n", "\n", "\n", 
   RowBox[{
    RowBox[{"uroots", "=", 
     RowBox[{"Solve", "[", 
      RowBox[{
       RowBox[{"Flatten", "[", "eqns", "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"u1", ",", "u2", ",", "u3", ",", "u4"}], "}"}]}], "]"}]}], 
    ";"}], "\n", "\n", 
   RowBox[{
    RowBox[{"s1", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "+", 
          RowBox[{"Sqrt", "[", 
           RowBox[{"1", "+", 
            RowBox[{"2", " ", 
             RowBox[{"h", "^", "2"}], " ", "En"}]}], "]"}]}], ")"}], "/", 
        RowBox[{"h", "^", "2"}]}], " ", "+", 
       RowBox[{"u1", "/", 
        RowBox[{"c", "^", "2"}]}]}], "/.", 
      RowBox[{"uroots", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "1"}], "]"}], "]"}]}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"s2", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"Sqrt", "[", 
           RowBox[{"1", "+", 
            RowBox[{"2", " ", 
             RowBox[{"h", "^", "2"}], " ", "En"}]}], "]"}]}], ")"}], "/", 
        RowBox[{"h", "^", "2"}]}], " ", "+", 
       RowBox[{"u2", "/", 
        RowBox[{"c", "^", "2"}]}]}], "/.", 
      RowBox[{"uroots", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "2"}], "]"}], "]"}]}]}], ";"}], "\n", "\n", 
   RowBox[{
    RowBox[{"s3", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"u3", "+", 
        RowBox[{"u4", "*", 
         RowBox[{"c", "^", "2"}]}]}], "/.", 
       RowBox[{"uroots", "[", 
        RowBox[{"[", 
         RowBox[{"1", ",", "3"}], "]"}], "]"}]}], "/.", 
      RowBox[{"uroots", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "4"}], "]"}], "]"}]}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"aarray", "[", 
     RowBox[{"[", 
      RowBox[{"-", "1"}], "]"}], "]"}], ";"}], "\[IndentingNewLine]", "\n", 
   RowBox[{"Print", "[", "\"\<The three sroots are, \>\"", "]"}], 
   "\[IndentingNewLine]", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<\!\(\*SubscriptBox[\(s\), \(-\)]\)=\>\"", ",", "s1"}], "]"}],
    "\n", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<\!\(\*SubscriptBox[\(s\), \(+\)]\)=\>\"", ",", "s2"}], "]"}],
    "\[IndentingNewLine]", 
   RowBox[{"Print", "[", 
    RowBox[{"\"\<\!\(\*SubscriptBox[\(s\), \(3\)]\)=\>\"", ",", "s3"}], 
    "]"}]}]}]], "Input",
