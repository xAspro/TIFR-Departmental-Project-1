         RowBox[{"c", "^", "2"}]}]}], ",", 
       RowBox[{"2", "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"2", " ", "En", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "6"}], "+", 
             RowBox[{"7", " ", "\[Eta]"}]}], ")"}]}], ")"}], "/", 
         RowBox[{"c", "^", "2"}]}]}], ",", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"h", "^", "2"}]}], "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"2", " ", "En", " ", 
            RowBox[{"h", "^", "2"}], " ", 
            RowBox[{"(", 
             RowBox[{"1", "-", 
              RowBox[{"3", " ", "\[Eta]"}]}], ")"}]}], "+", 
           RowBox[{"5", " ", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"-", "2"}], "+", "\[Eta]"}], ")"}]}]}], ")"}], "/", 
         RowBox[{"c", "^", "2"}]}]}], ",", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"h", "^", "2"}], " ", 
          RowBox[{"(", 
           RowBox[{"8", "-", 
            RowBox[{"3", " ", "\[Eta]"}]}], ")"}]}], ")"}], "/", 
        RowBox[{"c", "^", "2"}]}]}], "}"}]}], ";"}], "\n", "\n", "\n", 
   RowBox[{
    RowBox[{"aarray2", "=", 
     RowBox[{"FullSimplify", "[", 
