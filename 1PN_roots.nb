In[3274]:= ClearAll["Global`*"]
coefficientsFromPolynomial[poly_, var_] := CoefficientList[poly, var]
polynomialFromCoefficients[coefficients_List, var_] := 
 Fold[#1  var + #2 &, 0, Reverse[coefficients]]
removeNegativeOrderTermsTillOrderN[poly_, var_, N_] := 
 Normal[Series[poly, {var, \[Infinity], 0}]] - 
  Normal[Series[poly, {var, \[Infinity], -(N + 1)}]]

