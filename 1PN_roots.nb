m = 1;
eqns = Table[aarray2[[i]] == sarray[[i]], {i, m + 2}];


removeNegativeOrderTermsTillOrderN[FullSimplify[eqns[[1, 1]]], 
  c, -2];
removeNegativeOrderTermsTillOrderN[FullSimplify[eqns[[1, 2]]], c, -2];

For[i = 1, i <= m + 2, i++,
  eqns[[i]] = 
    MapThread[
     Equal, {coefficientsFromPolynomial[
       removeNegativeOrderTermsTillOrderN[FullSimplify[eqns[[i, 1]]], 
        c, 2*m], c], 
      coefficientsFromPolynomial[
       removeNegativeOrderTermsTillOrderN[FullSimplify[eqns[[i, 2]]], 
        c, 2*m], c]}];
  ];


